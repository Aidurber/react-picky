    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "concurrently": "^4.1.2",
